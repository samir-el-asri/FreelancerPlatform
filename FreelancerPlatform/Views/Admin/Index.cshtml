@{
    ViewData["Title"] = "Admin";
    Layout = "_ALayout";
}

<br/><br/>
<div class="col-div-3">
	<div class="box">
		<p>@ViewData["FreelancersCount"]<br /><span>Freelancers</span></p>
		<i class="fa fa-users box-icon"></i>
	</div>
</div>
<div class="col-div-3">
	<div class="box">
		<p>@ViewData["ProjectsCount"]<br /><span>Projects</span></p>
		<i class="fa fa-list box-icon"></i>
	</div>
</div>
<div class="col-div-3">
	<div class="box">
		<p>@ViewData["RequestsCount"]<br /><span>Requests</span></p>
		<i class="fa fa-list-alt box-icon"></i>
