<!-- image home -->

<section class="content-banner">

    <div class="container">
        <div class="row d-flex justify-content-center">
            <div class="col-md-12">
                <div class="banner-con">
                    <p class="first-title"><span style="color: #e1d7d4;">Everyone's</span></p>
                    <p class="banner-des"><span style="color: #e1d7d4;">Can Start Manage his Project</span></p>
                    <p>
                        <span style="color: #e1d7d4;">
                            What are you waiting for ! With SKILLFULNESS you can <br /> find the best project that suit you
                            and Start work and <br />manage it.
                        </span>
                    </p>
                    <a href="" target="_blank" class="banner-btn">Get started</a>
                </div>
            </div>
        </div>
    </div>
</section>

<!-- end image home -->
<!-- Boxes -->
<section class="p-5">
    <div class="container">
        <div class="row text-center g-4">
            <div class="col-md">
